// 查询全字段
query.Type=TSQL
query.SQL=SELECT A.CASE_NO,A.RX_NO,A.SEQ_NO,A.OPT_USER,A.OPT_DATE,A.UPDATE_TIME,&
		 A.OPT_TERM,A.PRESRT_NO,A.REGION_CODE,A.MR_NO,A.ADM_TYPE,&
		 A.RX_TYPE,A.TEMPORARY_FLG,A.RELEASE_FLG,A.LINKMAIN_FLG,A.LINK_NO,&
		 A.ORDER_CODE,A.ORDER_DESC ||CASE &
					 WHEN TRIM(A.SPECIFICATION) IS NOT NULL OR TRIM(A.SPECIFICATION) <>''&
					 THEN '(' || A.SPECIFICATION || ')'&
					 ELSE ''&
					  END AS ORDER_DESC,A.SPECIFICATION,A.GOODS_DESC,A.ORDER_CAT1_CODE,A.MEDI_QTY,&
					      A.MEDI_UNIT,A.FREQ_CODE,A.ROUTE_CODE,A.TAKE_DAYS,A.DOSAGE_QTY,A.DOSAGE_UNIT,&
					      A.DISPENSE_QTY,A.DISPENSE_UNIT,A.GIVEBOX_FLG,A.OWN_PRICE,A.NHI_PRICE,&
					      A.DISCOUNT_RATE,A.OWN_AMT,A.AR_AMT,A.DR_NOTE,A.NS_NOTE,&
					      A.DR_CODE,A.ORDER_DATE,A.DEPT_CODE,A.DC_DR_CODE,A.DC_ORDER_DATE,&
					      A.DC_DEPT_CODE,A.EXEC_DEPT_CODE,A.SETMAIN_FLG,A.ORDERSET_GROUP_NO,A.ORDERSET_CODE,&
					      A.HIDE_FLG,A.RPTTYPE_CODE,A.OPTITEM_CODE,A.DEV_CODE,A.MR_CODE,&
					      A.FILE_NO,A.DEGREE_CODE,A.URGENT_FLG,A.INSPAY_TYPE,A.PHA_TYPE,&
					      A.DOSE_TYPE,A.PRINTTYPEFLG_INFANT,A.EXPENSIVE_FLG,A.CTRLDRUGCLASS_CODE,A.PRESCRIPT_NO,&
					      A.ATC_FLG,A.SENDATC_DATE,A.RECEIPT_NO,A.BILL_FLG,A.BILL_DATE,&
					      A.BILL_USER,A.PRINT_FLG,A.REXP_CODE,A.HEXP_CODE,A.CONTRACT_CODE,&
					      A.CTZ1_CODE,A.CTZ2_CODE,A.CTZ3_CODE,A.PHA_CHECK_CODE,A.PHA_CHECK_DATE,&
					      A.PHA_DOSAGE_CODE,A.PHA_DOSAGE_DATE,A.PHA_DISPENSE_CODE,A.PHA_DISPENSE_DATE,A.NS_EXEC_CODE,&
					      A.NS_EXEC_DATE,A.NS_EXEC_DEPT,A.DCTAGENT_CODE,A.DCTEXCEP_CODE,A.DCT_TAKE_QTY,&
					      A.PACKAGE_TOT,A.AGENCY_ORG_CODE,A.DCTAGENT_FLG,A.DECOCT_CODE,A.EXEC_FLG,A.RECEIPT_FLG,A.CAT1_TYPE,&
					      A.MED_APPLY_NO,A.BILL_TYPE,A.PHA_RETN_CODE,A.BUSINESS_NO,B.NHI_CODE_O,B.NHI_CODE_E,B.NHI_CODE_I,   &
					      CASE WHEN A.IS_PRE_ORDER = 'Y' THEN 'Y' ELSE 'N' END AS IS_PRE_ORDER, MEM_PACKAGE_ID, A.EXEC_DR_CODE  &
					      FROM OPD_ORDER A,SYS_FEE B &
	   WHERE A.ORDER_CODE=B.ORDER_CODE AND A.RELEASE_FLG <> 'Y' ORDER BY A.RX_TYPE,A.RX_NO,A.ORDERSET_GROUP_NO,A.SEQ_NO 
query.item=CASE_NO;RX_NO;SEQ_NO;RECEIPT_NO
query.CASE_NO=A.CASE_NO=<CASE_NO>
query.RX_NO=A.RX_NO=<RX_NO>
query.SEQ_NO=A.SEQ_NO=<SEQ_NO>
query.RECEIPT_NO=A.RECEIPT_NO=<RECEIPT_NO>
query.Debug=N

// 医保查询全字段
queryIns.Type=TSQL
queryIns.SQL=SELECT A.CASE_NO,A.RX_NO,A.SEQ_NO,A.OPT_USER,A.OPT_DATE,&
		 A.OPT_TERM,A.PRESRT_NO,A.REGION_CODE,A.MR_NO,A.ADM_TYPE,&
		 A.RX_TYPE,A.TEMPORARY_FLG,A.RELEASE_FLG,A.LINKMAIN_FLG,A.LINK_NO,&
		 A.ORDER_CODE,A.ORDER_DESC ||CASE &
					 WHEN TRIM(A.SPECIFICATION) IS NOT NULL OR TRIM(A.SPECIFICATION) <>''&
					 THEN '(' || A.SPECIFICATION || ')'&
					 ELSE ''&
					  END AS ORDER_DESC,A.SPECIFICATION,A.GOODS_DESC,A.ORDER_CAT1_CODE,A.MEDI_QTY,&
					      A.MEDI_UNIT,A.FREQ_CODE,A.ROUTE_CODE,A.TAKE_DAYS,A.DOSAGE_QTY,A.DOSAGE_UNIT,&
					      A.DISPENSE_QTY,A.DISPENSE_UNIT,A.GIVEBOX_FLG,A.OWN_PRICE,A.NHI_PRICE,&
					      A.DISCOUNT_RATE,A.OWN_AMT,A.AR_AMT,A.DR_NOTE,A.NS_NOTE,&
					      A.DR_CODE,A.ORDER_DATE,A.DEPT_CODE,A.DC_DR_CODE,A.DC_ORDER_DATE,&
					      A.DC_DEPT_CODE,A.EXEC_DEPT_CODE,A.SETMAIN_FLG,A.ORDERSET_GROUP_NO,A.ORDERSET_CODE,&
					      A.HIDE_FLG,A.RPTTYPE_CODE,A.OPTITEM_CODE,A.DEV_CODE,A.MR_CODE,&
					      A.FILE_NO,A.DEGREE_CODE,A.URGENT_FLG,A.INSPAY_TYPE,A.PHA_TYPE,&
					      A.DOSE_TYPE,A.PRINTTYPEFLG_INFANT,A.EXPENSIVE_FLG,A.CTRLDRUGCLASS_CODE,A.PRESCRIPT_NO,&
					      A.ATC_FLG,A.SENDATC_DATE,A.RECEIPT_NO,A.BILL_FLG,A.BILL_DATE,&
					      A.BILL_USER,A.PRINT_FLG,A.REXP_CODE,A.HEXP_CODE,A.CONTRACT_CODE,&
					      A.CTZ1_CODE,A.CTZ2_CODE,A.CTZ3_CODE,A.PHA_CHECK_CODE,A.PHA_CHECK_DATE,&
					      A.PHA_DOSAGE_CODE,A.PHA_DOSAGE_DATE,A.PHA_DISPENSE_CODE,A.PHA_DISPENSE_DATE,A.NS_EXEC_CODE,&
					      A.NS_EXEC_DATE,A.NS_EXEC_DEPT,A.DCTAGENT_CODE,A.DCTEXCEP_CODE,A.DCT_TAKE_QTY,&
					      A.PACKAGE_TOT,A.AGENCY_ORG_CODE,A.DCTAGENT_FLG,A.DECOCT_CODE,A.EXEC_FLG,A.RECEIPT_FLG,A.CAT1_TYPE,&
					      A.MED_APPLY_NO,A.BILL_TYPE,A.PHA_RETN_CODE,A.BUSINESS_NO,B.NHI_CODE_O,B.NHI_CODE_E,B.NHI_CODE_I &
					      FROM OPD_ORDER A,SYS_FEE_HISTORY B &
	      WHERE A.ORDER_CODE=B.ORDER_CODE AND A.RELEASE_FLG <> 'Y' AND A.ORDER_DATE BETWEEN &
              TO_DATE(B.START_DATE,'YYYYMMDDHH24MISS') AND TO_DATE(B.END_DATE,'YYYYMMDDHH24MISS') ORDER BY A.RX_TYPE,A.RX_NO,A.SEQ_NO
queryIns.item=CASE_NO;RX_NO;SEQ_NO;RECEIPT_NO
queryIns.CASE_NO=A.CASE_NO=<CASE_NO>
queryIns.RX_NO=A.RX_NO=<RX_NO>
queryIns.SEQ_NO=A.SEQ_NO=<SEQ_NO>
queryIns.RECEIPT_NO=A.RECEIPT_NO=<RECEIPT_NO>
queryIns.Debug=N

// 查询全字段:现金收费医嘱显示
queryFill.Type=TSQL
queryFill.SQL=SELECT   A.CASE_NO, A.RX_NO, A.SEQ_NO, A.OPT_USER, A.OPT_DATE, A.OPT_TERM,&
       		  A.PRESRT_NO, A.REGION_CODE, A.MR_NO, A.ADM_TYPE, A.RX_TYPE,&
         	  A.TEMPORARY_FLG, A.RELEASE_FLG, A.LINKMAIN_FLG, A.LINK_NO,&
                  A.ORDER_CODE,&
                  A.ORDER_DESC&
                  || CASE &
                WHEN TRIM (C.MAN_CHN_DESC) IS NOT NULL &
              	  OR TRIM (C.MAN_CHN_DESC) <> '' &
               	  THEN '*' || C.MAN_CHN_DESC &
            	   ELSE ''&
          	  END AS ORDER_DESC,&
		 A.SPECIFICATION, A.GOODS_DESC, A.ORDER_CAT1_CODE, A.MEDI_QTY,&
		 A.MEDI_UNIT, A.FREQ_CODE, A.ROUTE_CODE, A.TAKE_DAYS, A.DOSAGE_QTY,&
		 A.DOSAGE_UNIT, A.DISPENSE_QTY, A.DISPENSE_UNIT, A.GIVEBOX_FLG,&
		 A.OWN_PRICE, A.NHI_PRICE, A.DISCOUNT_RATE, A.OWN_AMT, A.AR_AMT,&
		 A.DR_NOTE, A.NS_NOTE, A.DR_CODE, A.ORDER_DATE, A.DEPT_CODE,&
		 A.DC_DR_CODE, A.DC_ORDER_DATE, A.DC_DEPT_CODE, A.EXEC_DEPT_CODE,&
		 A.SETMAIN_FLG, A.ORDERSET_GROUP_NO, A.ORDERSET_CODE, A.HIDE_FLG,&
		 A.RPTTYPE_CODE, A.OPTITEM_CODE, A.DEV_CODE, A.MR_CODE, A.FILE_NO,&
		 A.DEGREE_CODE, A.URGENT_FLG, A.INSPAY_TYPE, A.PHA_TYPE, A.DOSE_TYPE,&
		 A.PRINTTYPEFLG_INFANT, A.EXPENSIVE_FLG, A.CTRLDRUGCLASS_CODE,&
		 A.PRESCRIPT_NO, A.ATC_FLG, A.SENDATC_DATE, A.RECEIPT_NO, A.BILL_FLG,&
		 A.BILL_DATE, A.BILL_USER, A.PRINT_FLG, A.REXP_CODE, A.HEXP_CODE,&
		 A.CONTRACT_CODE, A.CTZ1_CODE, A.CTZ2_CODE, CTZ3_CODE,&
		 A.PHA_CHECK_CODE, A.PHA_CHECK_DATE, A.PHA_DOSAGE_CODE,&
		 A.PHA_DOSAGE_DATE, A.PHA_DISPENSE_CODE, A.PHA_DISPENSE_DATE,&
		 A.NS_EXEC_CODE, A.NS_EXEC_DATE, A.NS_EXEC_DEPT, A.DCTAGENT_CODE,&
		 A.DCTEXCEP_CODE, A.DCT_TAKE_QTY, A.PACKAGE_TOT, A.AGENCY_ORG_CODE,&
		 A.DCTAGENT_FLG, A.DECOCT_CODE, A.EXEC_FLG, A.RECEIPT_FLG,&
		 A.CAT1_TYPE, A.MED_APPLY_NO, A.BILL_TYPE, A.PHA_RETN_CODE,&
		 F.DOSE_CHN_DESC, '0.0%' AS AL,A.BUSINESS_NO &
   		 FROM OPD_ORDER A, SYS_MANUFACTURER C, SYS_FEE D, PHA_BASE E, PHA_DOSE F &
   		WHERE A.ORDER_CODE = D.ORDER_CODE(+) &
		     AND D.MAN_CODE = C.MAN_CODE(+) &
		     AND A.ORDER_CODE = E.ORDER_CODE(+) &
		     AND E.DOSE_CODE = F.DOSE_CODE(+) ORDER BY RX_TYPE,RX_NO,SEQ_NO 
queryFill.item=CASE_NO;RX_NO;SEQ_NO;RECEIPT_NO 
queryFill.CASE_NO=CASE_NO=<CASE_NO>
queryFill.RX_NO=RX_NO=<RX_NO>
queryFill.SEQ_NO=SEQ_NO=<SEQ_NO>
queryFill.RECEIPT_NO=RECEIPT_NO=<RECEIPT_NO>
queryFill.Debug=N


