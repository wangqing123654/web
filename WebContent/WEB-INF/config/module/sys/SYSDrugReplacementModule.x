//根据 order_code,REPLACE_ORDER_CODE 删除数据
deletedata.Type=TSQL
deletedata.SQL=DELETE PHA_REDRUG  WHERE ORDER_CODE = <ORDER_CODE>  AND REPLACE_ORDER_CODE=<REPLACE_ORDER_CODE>
