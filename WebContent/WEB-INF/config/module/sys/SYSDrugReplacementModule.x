//根据 order_code,REPLACE_ORDER_CODE更新数据
updatedata.Type=TSQL
updatedata.SQL=UPDATE PHA_REDRUG SET SEQ=<SEQ>,DESCRIPTION=<DESCRIPTION>,OPT_USER =<OPT_USER>,OPT_DATE=SYSDATE,OPT_TERM=<OPT_TERM> WHERE ORDER_CODE=<ORDER_CODE> AND REPLACE_ORDER_CODE=<REPLACE_ORDER_CODE> 
 WHERE ORDER_CAT1_CODE = <ORDER_CAT1_CODE> 
