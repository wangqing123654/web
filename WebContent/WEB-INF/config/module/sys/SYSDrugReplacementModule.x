Module.item=selectall;selectdata;deletedata;insertdata;updatedata;existsPosition;selectbyordercode;selectbyreplaceordercode;selectsysfee

// 根据 order_code,REPLACE_ORDER_CODE 查询数据  selectbyreplaceordercode
selectdata.Type=TSQL
selectdata.SQL=SELECT A.SEQ AS SEQ,A.ORDER_CODE AS ORDER_CODE,A.REPLACE_ORDER_CODE AS REPLACE_ORDER_CODE,B.ORDER_DESC AS ORDER_DESC,C.ORDER_DESC AS REPLACE_ORDER_DESC,A.DESCRIPTION AS DESCRIPTION,A.OPT_USER AS OPT_USER ,A.OPT_DATE AS OPT_DATE FROM PHA_REDRUG A ,SYS_FEE B,SYS_FEE C WHERE A.ORDER_CODE=<ORDER_CODE> AND A.REPLACE_ORDER_CODE=<REPLACE_ORDER_CODE> AND B.ORDER_CODE=A.ORDER_CODE AND C.ORDER_CODE=A.REPLACE_ORDER_CODE ORDER BY A.ORDER_CODE, A.SEQ
selectdata.Debug=N

