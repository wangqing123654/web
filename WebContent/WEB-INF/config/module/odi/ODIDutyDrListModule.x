//删除收费等级基本档数据
delete.Type=TSQL
delete.SQL=DELETE ODI_DUTYDRLIST WHERE STATION_CODE=<STATION_CODE> AND DR_CODE=<DR_CODE> AND DEPT_CODE=<DEPT_CODE> AND SEQ=<SEQ>
delete.Debug=N
//查询收费等级基本档数据
query.Type=TSQL
query.SQL=SELECT DEPT_CODE,STATION_CODE,DR_CODE,SEQ,OPT_USER,OPT_DATE,OPT_TERM FROM ODI_DUTYDRLIST ORDER BY STATION_CODE
