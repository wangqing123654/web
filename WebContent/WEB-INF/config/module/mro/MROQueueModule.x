//add by wangbin 清空客服建档状态标记
updateSysPatInfoBookBuild.Type=TSQL
updateSysPatInfoBookBuild.SQL=UPDATE SYS_PATINFO SET BOOK_BUILD=<BOOK_BUILD> WHERE MR_NO=<MR_NO>
updateSysPatInfoBookBuild.Debug=N

//add by wangbin 20140911 查询病案夹数据
selectMroPrintData.Type=TSQL
selectMroPrintData.SQL=SELECT A.MR_NO,A.PAT_NAME,A.BIRTH_DATE,A.CELL_PHONE,A.TEL_HOME,B.GUARDIAN1_PHONE,A.IPD_NO, &
								(SELECT B.CHN_DESC FROM SYS_DICTIONARY B WHERE B.GROUP_ID='SYS_SEX' AND B.ID = A.SEX_CODE) AS SEX, &
								(SELECT B.CHN_DESC FROM SYS_DICTIONARY B WHERE B.GROUP_ID='SYS_MARRIAGE' AND B.ID = A.MARRIAGE_CODE) AS MARRIAGE, &
								(SELECT B.CHN_DESC FROM SYS_DICTIONARY B WHERE B.GROUP_ID='SYS_NATION' AND B.ID = A.NATION_CODE) AS NATION, &
								(SELECT B.CHN_DESC FROM SYS_DICTIONARY B WHERE B.GROUP_ID='SYS_SPECIES' AND B.ID = A.SPECIES_CODE) AS SPECIES &
								FROM SYS_PATINFO A, MEM_PATINFO B &
								WHERE A.MR_NO = B.MR_NO(+)
selectMroPrintData.item=MR_NO
selectMroPrintData.MR_NO=A.MR_NO=<MR_NO>
selectMroPrintData.Debug=N
